			h2
				full-width()
				font-size: 14px
				margin-bottom: 5px
			&.man
				#q_0, #q_4
					.counter
						height: 14px
						line-height: 14px
				#q_3
					.counter
						height: 24px
						line-height: 24px
				#q_1
					.counter
						height: 37px
						line-height: 37px
				#q_2
					.counter
						height: 24px
						line-height: 24px
			&.woman
				#q_0, #q_1, #q_2, #q_4, #q_6
					.counter
						height: 24px
						line-height: 24px
				#q_3, #q_5
					.counter
						height: 24px
						line-height: 24px
				#q_7
					.counter
						height: 24px
						line-height: 24px
			#result_block
				margin-top: 5px
				p
					font-size: 12px
				.button_block
					height: 150px
				#balls
					font-size: 22px
				#printButton, #testRepeat
					font-size: 13px
					padding: 0 20px
				#testRepeat
					margin-top: 10px
			.questions
				h3
					font-size: 12px !important
					background-size: auto 100% !important
					background-color: #d11414 !important
					line-height: 12px
					margin: 15px 0 5px
					br
						display: none
					.counter
						font-size: 28px
						width: 20px
						text-indent: 3px
				ol
					li
						line-height: 20px
						full-width()
						input
							width: 12px
							border-color: white
							margin-top: 2px
						label
							width: 92%
							float: right
							font-size: 12px
							line-height: 11px
							margin: 4px 0
							color: #ce181f
	@media (max-width: 540px)
		.test_content
			&.man
				#q_0, #q_4
					.counter
						height: 24px
						line-height: 24px
				#q_3
					.counter
						height: 37px
						line-height: 37px
				#q_1
					.counter
						height: 62px
						line-height: 62px
				#q_2
					.counter
						height: 52px
						line-height: 52px
			&.woman
				#q_0, #q_1, #q_2, #q_4, #q_6
					.counter
						height: 37px
						line-height: 37px
				#q_3, #q_5
					.counter
						height: 24px
						line-height: 24px
				#q_7
					.counter
						height: 48px
						line-height: 48px
	.locator-frame
		@media (max-width: 770px)
			background: white
			full-width()
			padding: 8px
			box-shadow: 1px 1px 2px rgba(8, 10, 14, 0.22)
			margin-top: 14px
			a.doctor
				margin-bottom: 0 !important
				width: 50%
				font-size: 14px
				line-height: 35px
				height: 35px
.test_container
	@media (max-width: 770px)
		padding: 0 !important
#test-man
	#result_block
		margin-top: 40px
		full-width()
	#q_3, #q_00
		h3
			b("text_md.png", top right no-repeat)
		.counter
			height: 78px
	#q_00
		h3
			padding-left: 30px
			@media screen and (max-width: 1024px)
				padding-left: 10px
	#q_4, #q_0
		h3
			b("test_sm.png", top right no-repeat)
		.counter
			height: 46px
			@media screen and (max-width: 1024px)
				height: 54px
	#q_1
		h3
			font-size: 22px
	#q_2, #q_1
		h3
			b("test_bg.png", top right no-repeat)
		.counter
			height: 82px
		@media screen and (max-width: 1024px)
			.counter
				height: 100px
#more-link
	full-width()
	background: white
	margin-bottom: 14px
	text-transform: uppercase
	padding: 0 8px
	display: none
	line-height: 40px
	font-size: 24px
	box-shadow: 1px 1px 2px rgba(8, 10, 14, 0.22)
	svg
		float: right
		margin-top: 12px
	@media (max-width: 770px)
		display: block
#promo
	full-width()
	overflow: hidden
	@media (max-width: 770px)
		padding: 0 8px
		margin-bottom: -1*14px
		.block
			&.first
				.image
					full-width()
					height: 165px
				.content
					full-width()
					padding: 8px 8px 14px 8px !important
					.title
						font-size: 18px
						line-height: 18px
						margin-bottom: 10px
						position: relative
						top: 20px
						left: 0
						transform(none)
					.text
						font-size: 14px
						line-height: 15px
						display: block
	&.woman
		.block
			&.big
				height: 526px
				@media (max-width: 770px)
					height: auto
				#symtpoms-articles &
					height: 380px
					.title
						font-size: 34px
					.text
						font-size: 18px
			&.medium
				height: 256px
				@media (max-width: 770px)
					height: auto
				#symtpoms-articles &
					height: 183px
					.title
						font-size: 21px
						line-height: 20px
					.text
						font-size: 13px
			.text
				.bb
					display: inline-block
					font-weight: 300
					line-height: 100px !important
.links
	position absolute
	bottom 40px
	left 0
	display none
	@media (min-width: 770px)
		display block
	@media (min-width: 1024px)
		right 0
		left auto
		bottom 0px
	a.border
		position relative
		margin-left 20px
		display inline-block
		&:after
			box-shadow inset 0 0 0 1px rgba(255,255,255,.7)
			display inline-block
			border-radius 5px
			content ""
			position absolute
			top 0
			left 0
			width 100%
			height 100%
#search
	float right
	margin-bottom -10px
	position relative
	width 300px
	max-width 100%
	input[type="text"]
		border 2px solid #eb222e
		padding 0 30px 0 10px
		height 30px
		line-height 30px
		font-size 14px
		margin-bottom 5px
		width 100%
		float left
		font-weight: bold
		.dark &
			background transparent
			border-color white
			color white
	label
		font-size 14px
		font-weight: normal
	button
		background transparent
		border 0
		position absolute
		width 28px
		height 28px
		right 12px
		top 1px
		svg
			display inline-block
			vertical-align: middle
			position relative
			top -2px
			.dark &
				path
					fill white
			
	@media (max-width: 770px)
		display none

.modal-sm
	width 450px
	
#question
	.success
		display none
		h3
			margin-bottom 0
	h3
		margin-bottom 30px
	input[type="text"], textarea
		width 100%
		resize none
		border 1px solid #b5b5b5
		margin-bottom 15px
		padding 5px 10px
		&.error
			border-color #eb222e
	input
		&[type="text"]
			line-height 30px
			height 30px
		&[type="submit"]
			border 0
			color white
			font-weight: bold
			margin-top 10px
			display inline-block
			background-image: linear-gradient(bottom left, rgba(155, 29, 33, 0.8) -18.85%, rgba(233, 33, 45, 0.8) 81.15%)
			line-height: 30px
			padding: 2px 29px 3px
		

.search
	min-height 400px
	form
		display none
	h1
		margin-bottom 30px
		font-size 40px
	.search-page
		font-size 15px
		a
			color #eb222e
			text-decoration: underline
		.page__divider
			margin 30px 0
			border-bottom 1px solid #ccc
		&__text
			width 100%
			margin-top 10px
		&__title
			font-size 25px
			
		&__text, &__sort
			font-size 14px
		small
			width 100%
			opacity .6
			display inline-block
