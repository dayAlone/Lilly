$orange = #ffba00

full-width()
	float: left
	width: 100%
	position: relative

b(name=null, position=center center no-repeat, color=null)
	background: color url(/layout/images/name) position

@font-face
	font-family 'DINPro'
	src url("/layout/fonts/dinpro-light.ttf")
	font-weight 300
@font-face
	font-family 'DINPro'
	src url("/layout/fonts/dinpro-regular.ttf")
	font-weight normal
@font-face
	font-family 'DINPro'
	src url("/layout/fonts/dinpro-medium.ttf")
	font-weight 500
@font-face
	font-family 'DINPro'
	src url("/layout/fonts/dinpro-bold.ttf")
	font-weight 700
 
html, body
	min-height: 100%
body
	min-height: 100%
	full-width()
	max-width: 100%
	color: black
	font-family: "DINPro", "Helvetica Neue", Arial
	background: white
	font-size: 16px
	@media (max-width: 770px)
		background: #e5e5e5
		padding-top: 46px
		> .frame
			padding-top: 14px

a, a span, .shadow
	transition-property: background-color, opacity, color, border, box-shadow, z-index, font-size	
	transition-duration: 0.3s	
	color: black
	&:hover
		text-decoration: none

#panel
	@media (max-width: 770px)
		display: none
#overlay
	position: fixed
	top: 0
	left: 0
	background: transparent
	z-index: 60
	display: none
	width: 100%
	height: 100%
	.nav-open &
		display: block

#print
	.container
		width: 540px
		font-weight: 300
	.section, .title
		full-width()
		border-top: 1px solid #bcbcbc
		padding: 8px 15px
	.questions
		full-width()
		height: 700px
	.logo
		float: left
		margin-top: 15px
	.title
		border: 0
	h1
		font-size: 23px
		text-transform: uppercase
		full-width()
	p
		font-size: 15px
		margin-bottom: 0
		font-weight: 400
		full-width()
	h2
		text-transform: uppercase
		font-size: 18px
		color: #fc1e2b
		margin: 0 0 8px
		page-break-before: always
		full-width()
	h4
		font-weight: bold
		color: #f92334
		text-indent: -1*19px
		margin-left: 20px
		full-width()
	.line+h4
		margin-top: 20px
	.line
		full-width()
		border-bottom: 1px solid #bcbcbc
		height: 30px
	ul
		list-style: none
		padding: 0
		full-width()
		margin: 0
		li
			full-width()
			font-size: 12px
			line-height: 19px
			font-weight: 400
	.more
		full-width()
		margin-top: 40px
#pretest_block
	margin-bottom: 30px
	full-width()
#test-woman, #test-man
	h2
		text-transform: uppercase
		font-weight: 600
		margin-bottom: 20px
	p
		font-size: 15px
		full-width()
	p.small
		font-size: 14px
		margin-top: 25px
		color: #929292
	h3
		full-width()
		b("text_md.png", top right no-repeat)
		padding: 12px 0
		color: white
		margin-top: 20px
		margin-bottom: 15px
	.questions
		h3
			@media screen and (max-width: 1024px)
				font-size: 20px !important
				padding-right: 20px
	.counter
		float: left
		font-size: 50px
		line-height: 46px
		height: 78px
		font-weight: 700
		width: 50px
		text-align: center
		margin-right: 10px
		margin-left: 5px
	#result_block
		h3
			color: black
			background: transparent
			font-size: 37px
			font-weight: 600
	#balls
		font-size: 25px
		font-weight: 500
	#pow
		font-size: 14px
	#pow, #results_p
		float: left
		width: 60%
		@media screen and (max-width: 770px)
			width: 100%
	#printButton
		@media screen and (max-width: 770px)
			display: none !important
	#printButton, #testRepeat, #sendButton
		float: left
		clear: both
		margin-top: 20px
		display: block
		line-height: 45px
		padding: 0 30px
		text-transform: uppercase
		font-weight: 600
		color: white
		background: red
		span
			display: inline-block
			vertical-align: middle
			color: white
		&:hover
			opacity: 0.7
	#sendButton
		display: none
		padding: 0 20px
		font-size: 13px !important
		@media screen and (max-width: 770px)
			display: block
	#testRepeat
		background: #aaaaaa
	.button_block
		margin-top: -1*100px
		float: right
		width: 40%
		height: 200px
		b("ok.png")
		background-size: contain
		@media screen and (max-width: 770px)
			display: none
	#q_3, #q_5
		h3
			b("test_sm.png", top right no-repeat)
		.counter
			height: 46px
	#q_7
		h3
			b("test_bg.png", top right no-repeat)
		.counter
			height: 82px
	h3
		text-transform: uppercase
	ol
		padding-left: 0
		li
			list-style: none
			full-width()
	.questions
		full-width()
		h3
			@media (max-width: 1024px)
				background-color: #cf1920 !important
				background-size: auto 100% !important
	input[type="radio"]
		float: left
	label
		font-weight: 400
		float: right
		width: 95%
	h3.red
		background: transparent
		color: red
		margin-top: -1*30px
	a.doctor
		width: 209px
		height: 44px
		float: left
		b("doctor.png")
		color: white
		line-height: 44px
		font-weight: 600
		margin-bottom: 30px
		text-align: center
		text-transform: uppercase
		&:hover
			color: white
	@media (max-width: 770px)
		.test_frame
			full-width()
			background: white
			box-shadow: 1px 1px 2px rgba(8, 10, 14, 0.22)
			padding: 0 8px
			margin-top: 0px
			h2
				font-size: 15px
				line-height: 16px
				margin: 8px 0
			p
				font-size: 12px
			margin-bottom: 14px
		.test_content
			full-width()
			background: white
			box-shadow: 1px 1px 2px rgba(8, 10, 14, 0.22)
			padding: 0 8px 15px
			#footer
