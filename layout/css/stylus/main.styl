				padding: 0
				margin-bottom: -1*3px
				h3
					font-size: 14px
					margin: 0
					padding: 0
			.small
				full-width()
				font-size: 10px
				margin-top: 5px !important
				br
					display: none
			#pretest_block
				margin-bottom: 0
				h3
					background: transparent
					color: black
					padding-left: 0
					padding-right: 0
					color: #ce181f
					background: transparent !important
					padding: 5px 0
